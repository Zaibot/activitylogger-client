{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug Main Process",
      "type": "node",
      "request": "launch",
      "protocol": "legacy",
      "cwd": "${workspaceRoot}",
      "runtimeExecutable": "${workspaceRoot}/node_modules/.bin/electron.cmd",
      "program": "${workspaceRoot}/dist/app.js",
      "sourceMaps": true,
      "showAsyncStacks": true,
      "localRoot": "${workspaceRoot}/src/"
    }
  ]
}
