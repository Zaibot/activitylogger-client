{
  "name": "activitylogger-client",
  "version": "0.1.0",
  "description": "Activity Logger Client",
  "main": "dist/app.js",
  "homepage": "https://github.com/Zaibot/activitylogger-client",
  "scripts": {
    "start": "electron --debug .",
    "compile": "cross-env NODE_ENV=production webpack",
    "installer": "npm run compile && npm run pack",
    "pack": "cross-env NODE_ENV=production electron-builder",
    "dev": "nodemon -w src -w package.json -w webpack.config.js -e .json,.js,.html,.ts,.tsx,.less -i node_modules -x \"(webpack && npm start) || exit 0\"",
    "postinstall": "electron-builder install-app-deps",
    "protocol-update": "git pull --recurse-submodules && sh -c \"cd protocol; ./generate-descriptors.sh\"",
    "dist": "cross-env NODE_ENV=production build --publish onTagOrDraft"
  },
  "keywords": [],
  "author": "Zaibot Programs",
  "license": "ISC",
  "dependencies": {
    "@zaibot/ll-keyboard-hook-win": "^4.0.1",
    "@zaibot/ll-mouse-hook-win": "^4.0.1",
    "active-win": "^2.0.0",
    "async-file": "^2.0.2",
    "auto-launch": "^5.0.1",
    "bindings": "^1.3.0",
    "classnames": "^2.2.5",
    "electron-redux": "^1.3.0",
    "electron-squirrel-startup": "^1.0.0",
    "long": "^3.2.0",
    "nan": "^2.6.2",
    "nedb": "^1.8.0",
    "node-fetch": "^1.7.1",
    "node-memory-cache": "0.0.5",
    "node-notifier": "^5.1.2",
    "node-rsa": "^0.4.2",
    "protobufjs": "^6.8.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.5",
    "react-redux-pure": "^0.1.7",
    "redux-saga": "^0.15.6",
    "reselect": "^3.0.1",
    "uuid": "^3.1.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Zaibot/activitylogger-client.git"
  },
  "build": {
    "appId": "zaibot.activitylogger",
    "productName": "Activity Logger Desktop",
    "artifactName": "${name}-${version}-${channel}-${os}-${arch}.${ext}",
    "compression": "maximum",
    "win": {
      "target": "squirrel"
    },
    "directories": {
      "app": "."
    },
    "files": [
      "dist/*.js",
      "static/*.*"
    ],
    "protocols": {
      "name": "Zaibot's Activity Logger",
      "schemes": [
        "zaibot-al"
      ],
      "role": "Viewer"
    }
  },
  "devDependencies": {
    "@types/long": "^3.0.31",
    "@types/nedb": "^1.8.3",
    "@types/node": "^8.0.19",
    "@types/node-notifier": "0.0.28",
    "@types/react": "^16.0.0",
    "@types/react-dom": "^15.5.1",
    "@types/react-redux": "^4.4.47",
    "@types/redux": "^3.6.0",
    "@types/uuid": "^3.4.0",
    "@zaibot/css-reset": "^1.0.6",
    "classnames-loader": "^2.0.0",
    "cross-env": "^5.0.2",
    "css-loader": "^0.28.4",
    "electron": "^1.6.11",
    "electron-builder": "^19.18.1",
    "electron-builder-squirrel-windows": "^19.18.0",
    "electron-rebuild": "^1.6.0",
    "less": "^2.7.2",
    "less-loader": "^4.0.5",
    "style-loader": "^0.18.2",
    "ts-loader": "^2.3.2",
    "typescript": "^2.4.2",
    "webpack": "^3.4.1",
    "webpack-node-externals": "^1.6.0"
  }
}